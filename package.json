{
  "name": "gambit",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "parcel watch source-code/client/different-build-modes/main-game/index.html --dist-dir host-packages/main-game --no-content-hash",
    "prod": "cross-env NODE_ENV=production parcel build source-code/client/different-build-modes/main-game/index.html --dist-dir host-packages/main-game-prod --no-content-hash",
    "run-server": "cross-env NODE_ENV=development nodemon -q server.ts",
    "run-server-prod": "cross-env NODE_ENV=production nodemon -q server.ts",
    "dev-fightTest": "parcel serve source-code/client/different-build-modes/fight-test/index.html --dist-dir host-packages/fight-test",
    "dev-fightTest-serve": "webpack serve --config-name fightTest",
    "dev-localTest-serve": "webpack serve --config-name localTest",
    "dev-mainGame-serve": "webpack serve --config-name mainGame --hot",
    "prod-fightTest-build": "webpack --mode=production --config-name fightTest",
    "prod-localTest-build": "webpack --mode=production --config-name localTest",
    "prod-mainGame-build": "webpack --mode=production --config-name mainGame",
    "start": "npm run prod-mainGame-build & npm run prod-startServer",
    "build-all": "run-s prod-fightTest-build prod-localTest-build prod-mainGame-build",
    "dev-startServer-mon": "cross-env NODE_ENV=development nodemon server.ts",
    "dev-startServer": "cross-env NODE_ENV=development nodemon server.ts",
    "prod-startServer": "cross-env NODE_ENV=production node --max-old-space-size=30000 -r ts-node/register server.ts"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "mobx": "^6.6.2",
    "mobx-react": "^7.5.3",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.20",
    "parcel": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.5.7",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.7.0",
    "@parcel/transformer-raw": "^2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^1.4.36",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "process": "^0.11.10"
  }
}
