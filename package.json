{
  "name": "gambit",
  "version": "1.0.0",
  "scripts": {
    "single-player-dev": "webpack-dev-server --open --config config/single-player/webpack.single-player-dev.config.js",
    "single-player-host": "webpack --config config/single-player/webpack.single-player-host.config.js",
    "fight-ui-test": "webpack-dev-server --config config/webpack.fight-ui-test.config.js",
