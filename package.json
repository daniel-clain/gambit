    "manager-ui-test": "webpack-dev-server --config config/webpack.manager-ui-test.config.js",
    "main-game": "nodemon --inspect=0.0.0.0:1234 --require dotenv/config compiled-code/server/server.js ",
    "main-game-client-dev": "webpack-dev-server --config config/main-game/webpack.client.main-game.config.js",
    "main-game-client-host": "webpack --config config/main-game/webpack.client.main-game-host.config.js",
    "compile.server.main-game": "webpack --watch --config config/main-game/webpack.server.main-game.config.js",
    "test-page": "webpack-dev-server --config config/webpack.test.config.js"
  },
